<%- include("partials/header"); -%>

<div class="customer-table">
    <% Array.from(customers).forEach(function(customer){%>
        

    <ul>
        <img src="/images/image 1.png" alt="image">
        <li><label for="">Name : </label><%=customer.name%></li>
        <li><label>Email : </label><%=customer.email%></li>
        <li><label>Current balance : </label><%=customer.currBal%></li>
        <li><a href="/customers/<%= customer._id%>">Transfer money</a></li>
    </ul>
    <%}) %>
</div>


<%- include("partials/footer"); -%>
